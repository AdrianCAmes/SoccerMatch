@model IEnumerable<Entity.Calle>
@{
    ViewBag.Title = "Calle";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CCalle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NCalle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CDistrito.NDistrito)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CCalle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NCalle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CDistrito.NDistrito)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.CCalle }) |
